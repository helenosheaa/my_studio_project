<h4>Members</h4>

<div id="member-wrapper">
  <% @members.each do |member| %>

  <div class="member">
    <h3><%= member.first_name.capitalize %></h3>
    <p>Level: <%= member.level.level.capitalize %></p>
  </div>
  <a href="/members/<%= member.id %>/edit">Edit Member</a>
  <form action="/members/<%= member.id %>/delete" method="POST">
  <input type="submit" value="Delete Member"/></form>
  <% end %>
</div>
