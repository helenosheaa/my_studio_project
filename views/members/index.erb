<h1> Members </h1>
<table>
  <tr>
    <th>Member Name:</th>
    <th></th>
    <th>Level:</th>
    <th></th>
    <th>Update:</th>
  </tr>
  <%for member in @members %>
  <tr>
    <td> <a href="/members/<%= member.id  %>"><%= member.first_name.capitalize  %></td>
    <td>|</td>
    <td><%= member.level.level.capitalize  %></td>
    <td>|</td>
    <td><a href="/members/<%= member.id %>/edit">Edit Member</a></td>
  </tr>
  <% end %>
</table>
