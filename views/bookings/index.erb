<h1> Bookings </h1>
<div id= "booking-wrapper">
<table>
  <tr>
    <div class="booking">
    <th>Booking Number:</th>
    <th></th>
    <th>Member Name:</th>
    <th></th>
    <th>Class Name:</th>
  </tr>
  </div>
  <%for booking in @bookings %>
  <tr>
    <td> <a href="/bookings/<%= booking.id  %>"><%= booking.id  %></a> </td>
    <td>|</td>
    <td><%= booking.member.first_name  %></td>
    <td>|</td>
    <td><%= booking.yogaclass.name  %></td>
  </tr>
  <% end %>
</table>
</div>
